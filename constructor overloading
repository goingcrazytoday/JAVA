

class bike {
    int id;
    String name;
    int age;
    bike(int a,String b)
    {
         id=a;
         name=b;}
    bike(int a,String b,int c){
        id=a;
        name=b;
        age=c;
    }
    void display(){
        System.out.print(id+"  "+name+" "+age);
    }
    public static void main(String args[]) {
        bike d=new bike(10,"name");
        bike d2=new bike(20,"pinky",90);
        d.display();
        d2.display();
        
    }
}
